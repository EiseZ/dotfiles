{
  "languageserver": {
    "haskell": {
      "args": [
        "--lsp"
      ],
      "command": "haskell-language-server-wrapper",
      "filetypes": [
        "haskell",
        "lhaskell"
      ],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ]
    },
    "rust": {
      "command": "rust-analyzer",
      "filetypes": [
        "rust"
      ],
      "rootPatterns": [
        "Cargo.toml"
      ]
    }
  }
}
